{
  "name": "n8n-nodes-github-session-manager",
  "version": "0.2.0",
  "description": "Manage GitHub App session tokens in n8n and optionally rewire workflows to a freshly issued githubApi credential.",
  "keywords": ["n8n", "n8n-nodes", "n8n-community-node-package", "github", "jwt", "credentials", "session"],
  "homepage": "https://github.com/Jujuyeh/n8n-nodes-github-session-manager",
  "repository": {
    "type": "git",
    "url": "https://github.com/Jujuyeh/n8n-nodes-github-session-manager.git"
  },
  "bugs": {
    "url": "https://github.com/Jujuyeh/n8n-nodes-github-session-manager/issues"
  },
  "author": "Jujujyeh",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": ["dist"],
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "dev": "tsc -w -p tsconfig.build.json",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit -p tsconfig.build.json",
    "test": "vitest run",
    "test:watch": "vitest",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "axios": "^1.7.7"
  },
  "devDependencies": {
    "@types/node": "^20.12.12",
    "@typescript-eslint/eslint-plugin": "^8.11.0",
    "@typescript-eslint/parser": "^8.11.0",
    "eslint": "^9.11.1",
    "n8n-workflow": "^1.46.0",
    "typescript": "^5.6.3",
    "vitest": "^2.1.3"
  },
  "engines": { "node": ">=18.0.0" },
  "n8n": {
    "credentials": [
      "dist/credentials/GithubAppJwt.credentials.js"
    ],
    "nodes": [
      "dist/nodes/GithubSessionManager/GithubSessionManager.node.js"
    ]
  }
}